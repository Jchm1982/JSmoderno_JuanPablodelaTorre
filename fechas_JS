
const diaHoy = new Date();

let valor;

valor  = diaHoy;

valor = diaHoy.getFullYear();//Trae el a√±oo actual
valor = diaHoy.getMonth();//Trae el mes actual
valor = diaHoy.getMinutes();//Trae los minutos
valor = diaHoy.getHours();//Trae la hora
valor = diaHoy.getTime();//Trae los milisegundo que han trancurrido desde el 1 de enero de 1970




console.log(valor);
